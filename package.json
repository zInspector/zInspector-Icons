{
  "name": "@zinspector/icons",
  "version": "0.1.0",
  "type": "module",
  "description": "Shared icon library (Phosphor + custom) for web, React Native and raw SVG usage",
  "main": "dist/react-web.js",
  "module": "dist/react-web.js",
  "types": "dist/react-web.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-web.js",
      "require": "./dist/react-web.js",
      "types": "./dist/react-web.d.ts"
    },
    "./react-web": {
      "import": "./dist/react-web.js",
      "require": "./dist/react-web.js",
      "types": "./dist/react-web.d.ts"
    },
    "./react-native": {
      "import": "./dist/react-native.js",
      "require": "./dist/react-native.js",
      "types": "./dist/react-native.d.ts"
    },
    "./svg": {
      "import": "./dist/svg.js",
      "require": "./dist/svg.js",
      "types": "./dist/svg.d.ts"
    },
    "./react-web-only": {
      "import": "./dist/react-web-only.js",
      "require": "./dist/react-web-only.js",
      "types": "./dist/react-web-only.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "peerDependencies": {
    "phosphor-react": ">=2",
    "phosphor-react-native": ">=2",
    "react": ">=17",
    "react-dom": ">=17",
    "react-native": ">=0.72",
    "react-native-svg": ">=13"
  },
  "devDependencies": {
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "phosphor-react": "^1.4.1",
    "phosphor-react-native": "^3.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-native": "^0.82.1",
    "react-native-svg": "^15.15.0",
    "typescript": "^5.6.0"
  },
  "license": "MIT"
}
